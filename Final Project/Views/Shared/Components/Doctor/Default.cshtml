@model List<Doctor>

@foreach (Doctor doctors in Model)
{
    <div class="col-md-3 col-sm-6 col-xs-12 text-center aos-init aos-animate" data-aos="fade-up" data-aos-duration="500">
        <div class="doctor">
            <div class="img" style="background-image: url('@Url.Content($"~/img/{doctors.Image}")')">
            </div>
            <div class="doctor-info">
                <h3><a asp-controller="Doctor" asp-action="Index">@doctors.Name</a></h3>
                <span>Dental Hygienist</span>
                <ul class="doctor-social">
                    <li><a asp-controller="Doctor" asp-action="Index" href="#"><i class="fab fa-facebook-f"></i></a></li>
                    <li><a asp-controller="Doctor" asp-action="Index" href="#"><i class="fab fa-twitter"></i></a></li>
                    <li><a asp-controller="Doctor" asp-action="Index" href="#"><i class="fab fa-linkedin-in"></i></a></li>
                    <li><a asp-controller="Doctor" asp-action="Index" href="#"><i class="fab fa-instagram"></i></a></li>
                </ul>
            </div>
        </div>
    </div>
}

